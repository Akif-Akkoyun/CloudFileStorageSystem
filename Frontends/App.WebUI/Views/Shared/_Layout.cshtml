<!DOCTYPE html>
<html lang="en">
<head>
    <title>@(ViewData["Title"] is null ? string.Empty : ViewData["Title"] + " - ")Cloud File Storge System</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <partial name="Partials/_PageStyles" />
</head>
<body data-spy="scroll" data-target=".site-navbar-target" data-offset="300">

    <partial name="Partials/_Navbar" />

    @RenderBody()

    <partial name="Partials/_Footer" />

    <partial name="Partials/_PageScripts" />
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
